{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && nuxt build",
    "dev": "npx nuxi cleanup && nuxt dev --port=8888",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "prisma generate && nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.js"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.0.0-rc.3",
    "@sidebase/nuxt-auth": "^0.6.0-beta.3",
    "@types/node": "^20.5.7",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.7.0",
    "nuxt-quasar-ui": "^2.0.5",
    "nuxt3-leaflet": "^1.0.12",
    "prettier": "^3.0.3",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@intlify/core-base": "9.3.0-beta.22",
    "@intlify/shared": "9.3.0-beta.24",
    "@mdi/font": "^7.2.96",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@nuxtjs/cloudinary": "^2.3.4",
    "@prisma/client": "^5.2.0",
    "@quasar/extras": "^1.16.6",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.40.0",
    "formidable": "^3.5.1",
    "human-time": "^0.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "next-auth": "^4.21.1",
    "quasar": "^2.12.6",
    "sass": "^1.66.1",
    "translate-google": "^1.5.0",
    "url-pattern": "^1.0.3",
    "vue-i18n": "^9.4.1"
  }
}
